{
    "nick": "boogerbot",
    "channels": [ "#boogerbot" ],
    
    "dispatch": [
        {
            "match": "^source$",
            "say": "https://github.com/infrared/node-bot"
        },
        {
            "match": "^([0-1])+$",
            "module": "conversion",
            "func": "binary"
        },
        {
            "match": "^0x([0-9a-fA-F]+)$",
            "module": "conversion",
            "func": "hex"
        }
    ]
}
